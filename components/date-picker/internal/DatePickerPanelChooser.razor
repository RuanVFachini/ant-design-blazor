@namespace AntDesign.Internal
@typeparam TValue
@inherits AntDomComponentBase

<CascadingValue Value=@DatePicker>
    @if (IsShowDatePanel())
    {
        @if (DatePicker.IsShowTime)
        {
            <DatePickerDatetimePanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" />
        }
        else
        {
            <DatePickerDatePanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" />
        }
    }
    else if (IsShowQuarterPanel())
    {
        <DatePickerQuarterPanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" />
    }
    else if (IsShowWeekPanel())
    {
        <DatePickerDatePanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" IsWeek="@true" />
    }
    else if (IsShowMonthPanel())
    {
        <DatePickerMonthPanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" />
    }
    else if (IsShowYearPanel())
    {
        <DatePickerYearPanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" />
    }
    else if (IsShowDecadePanel())
    {
        <DatePickerDecadePanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" />
    }
    else if (IsShowTimePanel())
    {
        <DatePickerDatetimePanel TValue="TValue" PickerIndex="@PickerIndex" OnSelect="OnSelect" />
    }
</CascadingValue>
